                <mesh filename="package://unitree_a1_description/meshes/thigh.dae" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <joint name="back_right_thigh_to_back_right_hip" type="continuous">
        <origin xyz="0.0 0.08 0.0"/>
        <parent link="back_right_hip"/>
        <child link="back_right_thigh"/>
        <axis xyz="0.0 1 0.0"/>
        <limit lower="-0.785" upper="0.785" effort="0.0" velocity="0.0"/>
        
    </joint>

     <!-- Link between the back right thigh to the calf -->

