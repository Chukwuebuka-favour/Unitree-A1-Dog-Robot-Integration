<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="unitreeA1">

    <link name="base_link">
    </link>

    <link name="trunk">
        <visual>
            <geometry>
                <mesh filename="package://unitree_a1_description/meshes/trunk.dae" scale="1 1 1"/>
            </geometry>
            <material name="trunk_color">
                <color rgba="0.0 0.0 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="base_link_to_trunk" type="fixed">
        <origin xyz="0.0 0.0 0.7"/>
        <parent link="base_link"/>
        <child link="trunk"/>
    </joint>

    <!-- Link between the trunk(Body of robot) to the front right hip -->

    <link name="front_right_hip">
        <visual>
            <geometry>
                <mesh filename="package://unitree_a1_description/meshes/hip.dae" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <joint name="front_right_hip_to_trunk" type="revolute">
        <origin xyz="0.17 0.05 0.0"/>
        <parent link="trunk"/>
        <child link="front_right_hip"/>
        <axis xyz="1 0 1"/>
        <limit lower="-0.5" upper="0.5" effort="0.0" velocity="0.0"/>
    </joint>


    <!-- Link between the front right hip to the front right thigh -->

    <link name="front_right_thigh">
        <visual>
            <origin rpy="0.0 0.6 0.0"/>
            <geometry>
                <mesh filename="package://unitree_a1_description/meshes/thigh.dae" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <joint name="front_right_thigh_to_front_right_hip" type="continuous">
        <origin xyz="0.0 0.08 0.0"/>
        <parent link="front_right_hip"/>
        <child link="front_right_thigh"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- Link between the front right thigh to the calf -->

    <link name="front_right_calf">
        <visual>
            <origin rpy="0.0 -0.785 0.0"/>
            <geometry>
                <mesh filename="package://unitree_a1_description/meshes/calf.dae" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <joint name="front_right_calf_to_front_right_thigh" type="revolute">
        <origin xyz="-0.115 0.0 -0.166"/>
        <parent link="front_right_thigh"/>
        <child link="front_right_calf"/>
        <axis xyz="0 1 0"/>
        <limit lower="-0.785" upper="0.785" effort="0.0" velocity="0.0"/>
    </joint>

    <!-- Link between the trunk(Body of robot) to the back right hip -->

    <link name="back_right_hip">
        <visual>
            <origin rpy="0.0 3.14 0.0"/>
            <geometry>
                <mesh filename="package://unitree_a1_description/meshes/hip.dae" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <joint name="back_right_hip_to_trunk" type="revolute">
        <origin xyz="-0.17 0.05 0.0"/>
        <parent link="trunk"/>
        <child link="back_right_hip"/>
        <axis xyz="1 0 1"/>
        <limit lower="-0.5" upper="0.5" effort="0.0" velocity="0.0"/>
    </joint>

    <!-- Link between the back right hip to the back right thigh -->

